
.container {
	display: flex;
	flex-direction: column;
	align-items: center;
	padding: 40rpx;
	background-color: #F5F5F5; /* 更柔和的背景色 */
	background-image: url(/static/bg.jpg); /* 设置背景图片 */
	background-size: cover; /* 使图片覆盖容器 */
	background-position: center; /* 居中显示 */
	background-repeat: no-repeat; /* 不重复 */
}
.header {
  margin-bottom: 40px;
  text-align: center;
  background-color: #FFFFFF; /* 白色背景 */
  padding: 20rpx;
  border-radius: 30rpx;
  box-shadow: 0 0rpx 20rpx rgba(0, 0, 0, 0.1); /* 增加阴影效果 */
  width: 90%;
  background-image: url(data:image/png;base64,); /* 设置背景图片 */
  padding-bottom:160px;
  padding-right:0px;
  text-align: left; /* 设置文本左对齐 */
  border-bottom-width: 10px;
}
.title {
  font-size: 48rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  color: #2C3E50; /* 更深的文字颜色 */
}
.subtitle {
  font-size: 32rpx;
  color: #95A5A6; /* 灰色文字 */
}
.participation {
  font-size: 28rpx;
  color: #16A085; /* 绿色文字 */
  margin-top: 10rpx;
}
.grid {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  grid-gap: 30rpx; /* 增加间距 */
  width: 100%;
}
.grid-item {
  background-color: #fff;
  padding: 40rpx;
  border-radius: 25rpx; /* 圆角 */
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  text-align: center;
  box-shadow: 0 5rpx 15rpx rgba(0, 0, 0, 0.1); /* 增加阴影效果 */
  transition: -webkit-transform 0.2s;
  transition: transform 0.2s;
  transition: transform 0.2s, -webkit-transform 0.2s;
}
.grid-item:active {
  -webkit-transform: scale(0.98);
          transform: scale(0.98);
}
.small-text {
  font-size: 26rpx;
  color: #7F8C8D; /* 灰色小字 */
  margin-top: 10rpx;
}
.yellow { background-color: #FFEB3B;
}
.pink { background-color: #FFC1C1;
}
.green { background-color: #A8E6CF;
}
.blue { background-color: #B3E5FC;
}
.dark-green { background-color: #689F38;
}
.purple { background-color: #D1C4E9;
}


